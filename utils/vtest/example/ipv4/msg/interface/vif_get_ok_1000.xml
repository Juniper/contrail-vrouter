<?xml version="1.0"?>
<test>
    <test_name>Interface test - Get information about vif idx = 1 and compare it</test_name>
  <message>
        <vr_interface_req>
            <h_op>Get</h_op>
            <vifr_idx>1</vifr_idx>
        </vr_interface_req>
        <return>0</return>
        <expect>
            <vr_interface_req>
                <vifr_idx>1</vifr_idx>
                <vifr_name>1</vifr_name>
                <vifr_transport>2</vifr_transport>
                <vifr_vrf>0</vifr_vrf>
                <vifr_mtu>1514</vifr_mtu>
            </vr_interface_req>
        </expect>
    </message>

</test>

